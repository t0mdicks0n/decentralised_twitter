  "source": "pragma solidity 0.5.10;\n\nimport '../helpers/BaseStorage.sol';\n\ncontract UserStorage is BaseStorage {\n\n  // A mapping is similar to a hash table\n  // It's a structure which stores key-value pairs\n  mapping(uint => Profile) public profiles;\n\n  // A struct is like a class\n  struct Profile {\n    uint id;\n    bytes32 username;\n  }\n\n  // Index counter value\n  uint latestUserId = 0;\n\n  function createUser(bytes32 _username) public returns(uint) {\n    // Increment the global counter variable\n    latestUserId++;\n    // Instantiate a user/profile and store it at the key for\n    // it's index counter value\n    profiles[latestUserId] = Profile(latestUserId, _username);\n    // Return the user id\n    return latestUserId;\n  }\n\n}\n",
